{
  "name": "@macci-impact/max-package-playground",
  "version": "0.0.2",
  "scripts": {
    "build": "rm -rf dist && vite build -c src/landing-pages/vite.config.ts",
    "build:lp": "vite build -c src/landing-pages/vite.config.ts",
    "build-watch:lp": "vite build -c src/landing-pages/vite.config.ts --watch",
    "dev:lp": "vite -c src/landing-pages/vite.config.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest --run --passWithNoTests",
    "test:unit": "vitest --environment jsdom",
    "preview": "vite preview --port 4173",
    "saveBuildInfo": "node saveBuildInfo.ts",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "NODE_ENV=development storybook build -o dist",
    "type": "vue-tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "vue": "3.4.28"
  },
  "devDependencies": {
    "@impactinc/frontend-theme-storybook-addon": "^1.2.0",
    "@impactinc/frontend-theme-tokens": "^1.6.0",
    "@impactinc/frontend-vue-eslint-config": "^1.3.2",
    "@impactinc/ui-component-library": "4.2.1",
    "@storybook/addon-essentials": "8.6.7",
    "@storybook/addon-interactions": "8.6.7",
    "@storybook/addon-links": "8.2.9",
    "@storybook/builder-vite": "8.6.7",
    "@storybook/test": "8.6.7",
    "@storybook/vue3": "8.2.9",
    "@storybook/vue3-vite": "8.6.7",
    "@types/jsdom": "21.1.7",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "@vitejs/plugin-vue": "5.1.2",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.57.0",
    "jsdom": "^26.0.0",
    "msw": "2.3.5",
    "msw-storybook-addon": "2.0.3",
    "prettier": "3.3.3",
    "simple-git": "^3.25.0",
    "storybook": "^8.6.7",
    "terser": "^5.14.2",
    "typescript": "^5.5.4",
    "vite": "5.4.1",
    "vite-plugin-css-injected-by-js": "3.5.1",
    "vitest": "2.0.5",
    "vue-component-type-helpers": "2.0.29",
    "vue-tsc": "2.0.29"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
